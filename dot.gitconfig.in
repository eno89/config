# my gitconfig

[user]
	name = kana
	email = nicht@s8.xrea.com

[color]
	ui = auto
[core]
	pager = less -r
	excludesfile = ~/.gitignore
[alias]
	b = branch
	ba = branch -a
	br = branch -r
	ci = commit
	co = checkout
	d = diff HEAD
	dl = diff HEAD^
	ds = diff --cached
	du = diff
	l = log
	st = status
	staged = diff --cached
	unstaged = diff

# __END__
