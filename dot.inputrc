# $Id$

set bell-style none
set completion-ignore-case on
set convert-meta off
set editing-mode vi
set input-meta on
set mark-symlinked-directories on
set output-meta on
set show-all-if-ambiguous on
set visible-stats on

"\C- r": re-read-init-file
"\C- \C-r": re-read-init-file

# __END__
# vim: filetype=readline
