" Test: vim-ku-matcher-default
function s:test_ku_matcher_default_matches_p()  "{{{1
  Title "ku#matcher#default#matches_p()"

  let args = {'pattern': 'rail'}

  Assert !!ku#matcher#default#matches_p({'word': 'rail'}, args) ==> 1
  Assert !!ku#matcher#default#matches_p({'word': 'a rail'}, args) ==> 1
  Assert !!ku#matcher#default#matches_p({'word': 'rail gun'}, args) ==> 1
  Assert !!ku#matcher#default#matches_p({'word': 'a rail gun'}, args) ==> 1
  Assert !!ku#matcher#default#matches_p({'word': 'arailgun'}, args) ==> 1

  Assert !!ku#matcher#default#matches_p({'word': ''}, args) ==> 0
  Assert !!ku#matcher#default#matches_p({'word': 'reil gan'}, args) ==> 0
  Assert !!ku#matcher#default#matches_p({'word': 'RAILgun'}, args) ==> 0
endfunction




